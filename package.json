{
  "name": "hop-to-test",
  "version": "0.1.0",
  "displayName": "Hop to Test",
  "description": "Quickly hop between source files and their related test files",
  "publisher": "kbraun",
  "license": "MIT",
  "changelog": "CHANGELOG.md",
  "author": {
    "name": "Kevin Braun",
    "email": "kevin@kbraun.ca"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/kevinbraun/vscode-hop-to-test"
  },
  "bugs": {
    "url": "https://github.com/kevinbraun/vscode-hop-to-test/issues"
  },
  "homepage": "https://github.com/kevinbraun/vscode-hop-to-test",
  "icon": "extension-icon.png",
  "keywords": [
    "vscode",
    "extension",
    "test"
  ],
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:javascriptreact",
    "onLanguage:typescript",
    "onLanguage:typescriptreact",
    "onLanguage:vue"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "hopToTest.hop",
        "title": "Hop to Test/Source"
      }
    ],
    "keybindings": [
      {
        "command": "hopToTest.hop",
        "key": "ctrl+shift+t",
        "mac": "cmd+shift+t"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "vscode:prepublish": "npm run compile",
    "vscode:package": "vsce package",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "jest",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^18.0.0",
    "@types/vscode": "^1.74.0",
    "@typescript-eslint/eslint-plugin": "^8.46.4",
    "@typescript-eslint/parser": "^8.46.4",
    "@vscode/vsce": "latest",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "globals": "^16.5.0",
    "jest": "^29.5.0",
    "jiti": "^2.6.1",
    "prettier": "^3.6.2",
    "ts-jest": "^29.1.0",
    "typescript": "^4.9.4",
    "typescript-eslint": "^8.46.4"
  }
}
